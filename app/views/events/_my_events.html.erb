<h1> My Events </h1>

<table>
  <table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <% if event.host == current_user %>
          <td><%= link_to 'show', event %></td><br>
          <td><%= event.name %></td><br>
          <td><%= event.description %></td><br>
        <% end %>
    <% end %>
    </tbody>
</table>